close all; clear all;
load('data/studentdata1.mat');
t = [data.t];
dt = t(2:end) - t(1:end-1);
f = zeros(size(dt));

s = 0.0205*ones(4,1);

[b,a] = cheby1(4,5,0.4);
f = filter(b,a,dt,s);

% c=0;
% for i=1:numel(dt)
%    if c > 4
%        c = 0;
%    end
%    c = c+1;
%    s(c) = dt(i); 
%    temp = lowpass(s,0.7);
%    cheby1(n,Wn) 
%    f(i) = temp(c);
% end
    
s = 0.0205*ones(numel(dt)+1,1);
alpha = 0.8;
stm1 = 0.0205;
for i=1:numel(dt)
    s(i+1) = alpha*dt(i) + (1-alpha)*s(i);
end

plot(f)